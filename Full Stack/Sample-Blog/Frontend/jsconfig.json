{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@Components/*": [
        "src/Components/*"
      ],
      "@Global/*": [
        "src/Global/*"
      ],
      "@Layouts/*": [
        "src/Layouts/*"
      ],
      "@routes/*": [
        "src/routes/*"
      ],
      "@utils/*": [
        "src/utils/*"
      ],
      "@/*": [
        "src/*"
      ]
    }
  }
}